
extends layout

block content
    img(src='/images/WebsiteBackground.jpg', alt='christmas' style='width: 1000px; height: 550px; float: right') 


  table
    tr
      th Id
      th Username
      th Email
      th Age
      th
      th
    each item in user
      tr
        td
          a(href='/details/' + item.id) #{item.id}
        td #{item.username}
        td #{item.email}
        td #{item.age}
        td
          a(href='/edit/' + item.id) Edit
        td
          a(href='/delete/' + item.id) Delete